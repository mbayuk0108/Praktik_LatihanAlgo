package tugasalgo.t3;

/**
 *
 * @author Muhamad Bayu Khayatullah
 */
public class WHILE {
    public static Object[] i;
    public static void main(String[] args) {
        // for statement header includes initializitation,
        // loop-continuation condition and increment
        int i = 7; // Inisialisasi variabel sebelum while 
        while (i <= 77){// Kondisi perulangan 
            System.out.printf("%d", i); // Cetak nilai Counter 
            i += 7; // Peningkatan nilai counter 
        }
        System.out.println (); // output a newline 
    }   // end main
}  // end class ForCounter
